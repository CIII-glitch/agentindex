				{
					"name": "Search Agents",
					"request": {
						"method": "GET",
						"header": [],
						"url": {
							"raw": "{{baseUrl}}/search?q=crypto&limit=10",
							"host": ["{{baseUrl}}"],
							"path": ["search"],
							"query": [
								{
									"key": "q",
									"value": "crypto",
									"description": "Search query"
								},
								{
									"key": "category",
									"value": "",
									"description": "Filter by category",
									"disabled": true
								},
								{
									"key": "maxPrice",
									"value": "",
									"description": "Max price per call in USD",
									"disabled": true
								},
								{
									"key": "healthyOnly",
									"value": "true",
									"description": "Only healthy endpoints",
									"disabled": true
								},
								{
									"key": "limit",
									"value": "10",
									"description": "Max results"
								}
